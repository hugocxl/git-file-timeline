{
  "name": "githistory-web",
  "version": "2.0.0",
  "type": "module",
  "repository": "pomber/git-history",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check": "biome check .",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@pierre/diffs": "^1.0.0",
    "diff": "^7.0.0",
    "js-base64": "^3.7.7",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-swipeable": "^7.0.2",
    "rebound": "^0.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@storybook/react": "^8.4.7",
    "@types/diff": "^6.0.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}
